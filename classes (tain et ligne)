class ligne{
    constructor(NombreStations,TableauStations, Couleur){
        ligne.nbstations=NombreStations;
        ligne.tabstations=TableauStations;
        ligne.couleur=Couleur;
    }
}

class station{
    constructor(Dist_imp,Dist_ip1){
        this.avant=Dist_imp;
        this.apres=Dist_ip1;
    }
}
